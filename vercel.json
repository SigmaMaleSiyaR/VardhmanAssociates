{
  "version": 2,
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*",
      "has": [
        {
          "type": "host",
          "value": "admin.vardhmanassociates.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/$1",
      "has": [
        {
          "type": "host",
          "value": "vardhmanassociates.com"
        }
      ]
    }
  ]
}
